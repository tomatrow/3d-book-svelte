        "publish": "pnpm run lint && pnpm run package && pnpm publish ./package"
    },
    "devDependencies": {
        "@sveltejs/kit": "next",
        "autoprefixer": "^10.3.7",
        "cssnano": "^5.0.8",
        "postcss": "^8.3.9",
        "postcss-load-config": "^3.1.0",
        "postcss-nested": "^5.0.6",
        "prettier": "^2.4.1",
        "prettier-plugin-svelte": "^2.4.0",
        "svelte": "^3.42.6",
        "svelte-check": "^2.2.6",
        "svelte-preprocess": "^4.9.8",
        "svelte-typed-context": "^1.0.1",
        "svelte2tsx": "^0.4.7",
        "tailwindcss": "^2.2.16",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4",
        "vite": "^3.0.0"
    },
    "type": "module",
    "homepage": "https://github.com/tomatrow/3d-book-svelte#readme"
